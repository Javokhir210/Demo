<AnchorPane prefHeight="500.0" prefWidth="770.0" style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.smsfinal.logControl">
    <children>
      <AnchorPane layoutX="385.0" prefHeight="500.0" prefWidth="385.0">

         <children>
            <Label layoutX="123.0" layoutY="171.0" prefHeight="49.0" prefWidth="137.0" stylesheets="@../../../css_images/design.css" text="Sign In" textFill="#4f2773">
                <font>
                <Font name="Berlin Sans FB" size="48.0" />
                </font>
            </Label>
              <TextField fx:id="tfLogName" layoutX="101.0" layoutY="230.0" prefHeight="40.0" prefWidth="182.0" promptText="Username" styleClass="textfield-design" stylesheets="@../../../css_images/design.css" />
              <PasswordField fx:id="tfLogPass" layoutX="101.0" layoutY="283.0" prefHeight="40.0" prefWidth="182.0" promptText="Password" styleClass="textfield-design" stylesheets="@../../../css_images/design.css" />
              <Button fx:id="btLogin" layoutX="155.0" layoutY="334.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="75.0" styleClass="button-design" stylesheets="@../../../css_images/design.css" text="Login" textFill="WHITE">
                  <font>
                      <Font name="Arial Bold" size="14.0" />
                  </font>
            </Button>
              <Button fx:id="btSignGo" layoutX="113.0" layoutY="459.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="159.0" style="-fx-background-color: #fff;" text="Haven't registred yet" textFill="#1b9aea">
                  <font>
                      <Font name="Berlin Sans FB" size="14.0" />
                  </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
              </Button>
            <ComboBox fx:id="login_SignAs" accessibleText="a" layoutX="117.0" layoutY="388.0" prefWidth="150.0" promptText="Choose one" styleClass="selection" stylesheets="@../../../css_images/design.css" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="118.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../css_images/icons8-log-in-96.png" />
               </image>
            </ImageView>
            <Label layoutX="187.0" layoutY="371.0" styleClass="table-column" stylesheets="@../../../css_images/design.css" text="as" textFill="#6b00ff" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="-4.0" prefHeight="500.0" prefWidth="385.0" styleClass="login-bg" stylesheets="@../../../css_images/design.css">
         <children>
            <ImageView fitHeight="144.0" fitWidth="225.0" layoutX="80.0" layoutY="182.0" pickOnBounds="true" preserveRatio="true" styleClass="shadow">
               <image>
                  <Image url="@../../../css_images/logo.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
