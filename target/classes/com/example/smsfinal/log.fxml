        </ImageView>
        <Button fx:id="btSignGo" layoutX="366.0" layoutY="387.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="86.0" style="-fx-background-color: #87e2e6;" text="Sign up!" textFill="#b51414">
