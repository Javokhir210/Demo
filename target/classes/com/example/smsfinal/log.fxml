<AnchorPane prefHeight="500.0" prefWidth="750.0" style="-fx-background-color: #87e2e6;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.smsfinal.logControl">
    <children>
        <TextField fx:id="tfLogName" layoutX="291.0" layoutY="244.0" promptText="ex: Abcdef" />
        <PasswordField fx:id="tfLogPass" layoutX="291.0" layoutY="303.0" />
        <Label layoutX="198.0" layoutY="249.0" text="Username:">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font></Label>
        <Label layoutX="198.0" layoutY="308.0" text="Password:">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font></Label>
        <Button fx:id="btLogin" layoutX="291.0" layoutY="354.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="86.0" text="Login">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font></Button>
        <Label layoutX="283.0" layoutY="186.0" prefHeight="35.0" prefWidth="178.0" text="Login page">
            <font>
                <Font name="Algerian" size="30.0" />
            </font>
        </Label>
        <ImageView fitHeight="187.0" fitWidth="287.0" layoutX="222.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../css_images/Book.jpg" />
            </image>
        </ImageView>
        <Button fx:id="btSignGo" layoutX="366.0" layoutY="387.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="86.0" style="-fx-background-color: #87e2e6;" text="Sign up!" textFill="#b51414">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font>
        </Button>
        <Label layoutX="256.0" layoutY="390.0" prefHeight="20.0" prefWidth="116.0" text="Not registered?">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font>
        </Label>
      <ComboBox fx:id="login_SignAs" accessibleText="a" layoutX="468.0" layoutY="303.0" prefWidth="150.0" promptText="Choose one" />
      <Label layoutX="468.0" layoutY="269.0" text="Position:">
         <font>
            <Font name="Arial Bold" size="14.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
